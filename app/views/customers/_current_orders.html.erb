

<div class="current_orders">

              <% current_user.customer.orders.each do |order| %>
                <li>
                  <b><%= order.food.name %></b>
                  from
                  <%= order.food.business.name %>
                  at
                  <%= order.food.business.address %>
                  to pickup by
                  <%= order.food.pickup_end.strftime("%I:%M %p") %>
                  tonight
                </li>
              <% end %>

</div>
